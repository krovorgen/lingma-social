{
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "react/prop-types": 0,
    "max-len": "off",
    "indent": [
      "error",
      4
    ],
    "react/jsx-indent": [
      "error",
      4
    ],
    "react/react-in-jsx-scope": "off",
    "linebreak-style": 0,
    "dot-notation": "off",
    "react/destructuring-assignment": "off",
    "react/jsx-indent-props": "off"
  }
}
